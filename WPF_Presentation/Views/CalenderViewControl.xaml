<UserControl x:Class="WPF_Presentation.Views.CalenderViewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_Presentation.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#FFE9EFF8">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
     
        </Grid.ColumnDefinitions>
    
        <Grid>
            <Image x:Name="calenderImage" Grid.Column="0" Height="450" Width="800" VerticalAlignment="Center" Source="/Views/kalender.jpg"/>
        </Grid>
    </Grid>
</UserControl>
